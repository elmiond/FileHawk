<?xml version="1.0" encoding="UTF-8"?>
<!-- config.xml -->
<config>
	<actionsets>
		<actionset>
			<name>testmove</name>
			<actions>
				<action>
					<kind>move</kind>
					<destination>c:/images/</destination>
				</action>
			</actions>
		</actionset>
	</actionsets>
	<folders>
		<folder>
			<path>c:/test/</path>
			<actionsetname>testmove</actionsetname>
			<matchsetname>testregex</matchsetname>
		</folder>
		<folder>
			<path>o</path>
			<actionsetname></actionsetname>
			<matchsetname></matchsetname>
		</folder>
	</folders>
	<matches>
		<match>
			<name>testregex</name>
			<kind>regex</kind>
			<pattern>.*Image.*</pattern>
		</match>
	</matches>
</config>